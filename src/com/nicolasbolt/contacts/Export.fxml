<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.nicolasbolt.contacts.ExportController"
            prefHeight="400.0" prefWidth="600.0">
    <content>
        <GridPane vgap="10" hgap="10" fx:id="saveDialog">

            <Label text="Directory:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Button fx:id="openButton" text="Choose File Location" GridPane.rowIndex="1" GridPane.columnIndex="1" alignment="CENTER_LEFT"
                    onAction="#handleExportClick"/>

        </GridPane>
    </content>
</DialogPane>
