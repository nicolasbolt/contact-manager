<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.nicolasbolt.contacts.SettingsController"
            prefHeight="400.0" prefWidth="600.0">
    <content>
        <GridPane vgap="10" hgap="10" fx:id="saveDialog">
                <Label text="Hide Delete Warning" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                <CheckBox fx:id="deleteWarning" GridPane.rowIndex="0" GridPane.columnIndex="1" alignment="CENTER_LEFT"
                          onAction="#handleDeleteWarning"/>
        </GridPane>
    </content>
</DialogPane>
